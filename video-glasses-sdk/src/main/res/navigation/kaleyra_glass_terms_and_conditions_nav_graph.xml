<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/smartglass_nav_graph_terms_and_conditions"
    app:startDestination="@id/termsAndConditionsFragment">

    <fragment
        android:id="@+id/termsAndConditionsFragment"
        android:name="com.kaleyra.video_glasses_sdk.termsandconditions.TermsAndConditionsFragment"
        android:label="TermsAndConditionsFragment">
        <argument
            android:name="enableTilt"
            app:argType="boolean"
            android:defaultValue="false" />
        <argument
            android:name="termsAndConditions"
            app:argType="com.kaleyra.video_common_ui.termsandconditions.model.TermsAndConditions"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>


</navigation>