<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".userdataconsentagreement.GlassUserDataConsentAgreementActivity">

    <com.kaleyra.collaboration_suite_glass_ui.status_bar_views.StatusBarView
        android:id="@+id/kaleyra_status_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/kaleyra_content"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/kaleyra_status_bar">

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/kaleyra_nav_host_fragment"
            android:name="androidx.navigation.fragment.NavHostFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:defaultNavHost="true"
            app:navGraph="@navigation/kaleyra_glass_user_data_consent_agreement_nav_graph" />

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>